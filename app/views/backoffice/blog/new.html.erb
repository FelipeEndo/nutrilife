<%= simple_form_for [:backoffice, @blog], url: {action: "create"} do |f| %>
<%= f.input :title %>
<%= f.input :body, as: :text %>
<%= f.input :category_id, collection: @categories, label_method: :description %>
<%= f.input :images, as: :file, input_html: { multiple: true } %>
<%= f.button :submit, 'Criar' %>
<% end %>
